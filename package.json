{
  "name": "aut-web-cypress",
  "version": "1.0.0",
  "description": "Web Automation with Cypress",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:qa": "cypress run --env environment=qa",
    "cy:dev": "cypress run --env environment=dev",
    "cy:firefox": "cypress run --browser firefox",
    "cy:mobile": "cypress run --config viewportWidth=375,viewportHeight=667"
  },
