    "url": "https://gitlab.com/testever/aut-web-cypress/issues"
  },
  "homepage": "https://gitlab.com/testever/aut-web-cypress#readme",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^22.0.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.4",
    "cypress": "^14.2.1",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-on-fix": "^1.1.0",
    "cypress-xpath": "^2.0.1"
  }
}
